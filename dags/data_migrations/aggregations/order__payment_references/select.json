[
    {
        "$match": {
            "updatedAt": {
                "$gt": "{{ last_successful_dagrun_ds }}",
                "$lte": "{{ data_interval_end }}"
            },
            "zettle_payment_references": {
              "$exists": true,
              "$type": "array"
            }
        }
    },
    { "$unwind": "$zettle_payment_references" },
    {
        "$replaceRoot": {
            "newRoot": {
                "$mergeObjects": [
                  {
                    "order_id": "$_id",
                    "createdAt": "$createdAt",
                    "updatedAt": "$updatedAt"
                  },
                  "$zettle_payment_references"
                ]
            }
        }
    },
    { "$set": { "id": "$_id"}},
    {
        "$project": {
            "payment_references": 0,
            "_id": 0
        }
    },
    { "$skip": 0 },
    { "$limit": 5000}
]
