[
    {
        "$match": {
            "updatedAt": {
                "$gt": "{{ last_successful_dagrun_ds }}",
                "$lte": "{{ data_interval_end }}"
            }
        }
    },
    {
        "$unwind": {
            "path": "$possible_delivery_fees",
            "includeArrayIndex": "arrayIndex"
        }
    },
    {
        "$replaceRoot": {
            "newRoot": {
                "$mergeObjects": [
                  {
                    "order_id": "$_id",
                    "arrayIndex": "$arrayIndex"
                  },
                  "$possible_delivery_fees"
                ]
            }
        }
    },
    {
        "$set": {
            "id": {
                "$concat": [
                    {"$toString": "$order_id"},
                    "__",
                    {"$toString": "$_id"},
                    "__",
                    {"$toString": "$arrayIndex"}
                ]
            }
        }
    },
    {
        "$project": {
            "possible_delivery_fees": 0,
            "_id": 0
        }
    },
    { "$skip": 0 },
    { "$limit": 5000}
]
